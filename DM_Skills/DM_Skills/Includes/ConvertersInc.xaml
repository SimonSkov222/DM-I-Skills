<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Converters="clr-namespace:DM_Skills.Converters"
                    xmlns:local="clr-namespace:DM_Skills.Includes">

    <Converters:ProcentOfValueConverter x:Key="ProcentOfValueConvert"/>
    <Converters:TimeSpanToStringConverter x:Key="TimeToStringConvert"/>
    <Converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConvert" TrueValue="Visible" FalseValue="Collapsed"/>
    <Converters:ValueToThicknessConverter x:Key="HorizontalThicknessConvert" UseOnLeft="True" UseOnRight="True"/>
    <Converters:AutocompleteItemConverter x:Key="AutocompleteItemConvert"/>
    <Converters:TeamIDconvert x:Key="TeamIDconvert"/>
    <Converters:ToStringConverter x:Key="ToStringConvert"/>
</ResourceDictionary> 