<!-- Viser layout for tidtagning -->
<Window x:Class="DM_Skills.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DM_Skills"
        xmlns:Controls="clr-namespace:DM_Skills.Controls"
        xmlns:Models="clr-namespace:DM_Skills.Models"
        xmlns:View="clr-namespace:DM_Skills.Views"
        mc:Ignorable="d" x:Name="controller"
        FontFamily="Calibri"
        Background="{StaticResource Color_Background_Program}"
        Foreground="{StaticResource Color_Foreground_Program}"
        
        Title="MainWindow" Height="800" Width="1200">


    <Grid>
        <ScrollViewer Visibility="Visible">
            <StackPanel>
                <View:Forside/>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Window>
